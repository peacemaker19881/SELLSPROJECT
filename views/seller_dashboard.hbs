<h1>Your Products</h1>
<table>
  <thead><tr><th>Title</th><th>Price</th><th>Approved</th><th>Sold</th></tr></thead>
  <tbody>
    {{#each products}}
      <tr>
        <td>{{title}}</td>
        <td>{{price}}</td>
        <td>{{approved}}</td>
        <td>{{is_sold}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
